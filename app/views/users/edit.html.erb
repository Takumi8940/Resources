<h1>編集</h1>
<%= form_with model: @user, url: "/users/#{@user.id}",method: :patch do |form| %>
    <p>
        <%= form.label :nema, "名前" %>
        <%= form.text_field :nema %>
    </p>
    <p>
        <%= form.label :age, "年齢" %>
        <%= form.number_field :age %>
    </p>
    <div>
        <%= form.submit "更新" %>
    </div>
<% end %>