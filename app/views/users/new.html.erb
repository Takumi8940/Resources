<h1>新規投稿</h1>
<%= form_with model: @user do |form| %>
    <p>
        <%= form.label :nama, "お名前" %>
        <%= form.text_field :nama %>
    </p>
    <p>
        <%= form.label :age, "年齢" %>
        <%= form.number_field :age %>
    </p>
    <div>
        <%= form.submit "投稿" %>
    </div>
<% end %>